{
    "version": 2,
    "builds": [
      {
        "src": "index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.py"
      }
    ],
    "env": {
      "FLASK_SECRET_KEY": "@flask_secret_key",
      "OPENAI_API_KEY": "@openai_api_key"
    }
  }