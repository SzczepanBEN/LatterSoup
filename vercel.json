{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python",
      "config": { 
        "runtime": "python3.12",
        "buildCommand": "pip install -r requirements.txt && apt-get update && apt-get install -y wkhtmltopdf"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ]
}